{
  "name": "pc-portfolio",
  "version": "1.0.0",
  "description": "Portfolio for interested parties. ",
  "main": "server.js",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-mailer": "^0.3.1",
    "if-env": "^1.0.4",
    "nodemailer": "^4.7.0",
    "path": "^0.12.7"
  },
  "engines": {
    "node": "10.0.0",
    "npm": "5.6.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickcorcoran10/reactPortfolio.git"
  },
  "author": "Pat Corcoran",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/patrickcorcoran10/reactPortfolio/issues"
  },
  "homepage": "https://github.com/patrickcorcoran10/reactPortfolio#readme"
}
